{
  "DocumentationFiles": {
    "Files": [
      "**/*.md"
    ],
    "Exclude": [
      "**/packages/**",
      "**/bin/**",
      "**/obj/**"
    ]
  },
  "ResourceFolderNames": [
    ".attachments"
  ],
  "DocLinkChecker": {
    "AllowResourcesOutsideDocumentsRoot": true,
    "CheckForOrphanedResources": true,
    "CleanupOrphanedResources": false,
    "ValidatePipeTableFormatting": true,
    "ValidateExternalLinks": false,
    "ConcurrencyLevel": 5,
    "MaxHttpRedirects": 20,
    "WhitelistUrls": [
      "http://localhost",
      "https://www.nuget.org/*"
    ]
  }
}